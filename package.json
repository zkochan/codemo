{
  "name": "codemo",
  "version": "0.0.0-placeholder",
  "description": "Embeds console output to the code",
  "files": [
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "scripts": {
    "test": "mocha && npm run lint && mos test",
    "lint": "eslint lib/**/*.js example/**/*.js index.js",
    "commit": "git-cz",
    "coverage": "istanbul cover -x \"**/*.spec.js\" node_modules/mocha/bin/_mocha -- -R spec",
    "precoveralls": "istanbul cover -x \"**/*.spec.js\" node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && npm i coveralls@2",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postcoveralls": "rm -rf ./coverage",
    "md": "mos",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zkochan/codemo"
  },
  "keywords": [
    "example"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "zoltan.kochan@gmail.com",
    "url": "http://kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zkochan/codemo/issues"
  },
  "homepage": "https://github.com/zkochan/codemo#readme",
  "dependencies": {
    "acorn": "3.0.4",
    "callsites": "1.0.0",
    "cross-spawn-async": "2.2.2",
    "file-position": "0.0.0",
    "normalize-newline": "2.0.0",
    "normalize-path": "2.0.1",
    "rollup": "0.25.8",
    "rollup-plugin-babel": "2.4.0",
    "rollup-plugin-includepaths": "0.1.2",
    "source-map": "0.5.3"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "cz-conventional-changelog": "1.1.5",
    "eslint": "2.8.0",
    "eslint-config-standard": "5.1.0",
    "eslint-plugin-promise": "1.1.0",
    "eslint-plugin-standard": "1.3.2",
    "ghooks": "1.2.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4",
    "mos": "0.11.1",
    "semantic-release": "^4.3.5",
    "validate-commit-msg": "2.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "node ./node_modules/validate-commit-msg/index.js",
      "pre-commit": "npm test"
    }
  }
}
