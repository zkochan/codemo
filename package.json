{
  "name": "codemo",
  "version": "0.2.2",
  "description": "Embeds console output to the code",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "preview && mocha && npm run lint && mos test",
    "lint": "eslint src/**/*.js example/",
    "commit": "git-cz",
    "coverage": "istanbul cover -x \"**/*.spec.js\" node_modules/mocha/bin/_mocha",
    "precoveralls": "istanbul cover -x \"**/*.spec.js\" node_modules/mocha/bin/_mocha --report lcovonly && npm i coveralls@2",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postcoveralls": "rm -rf ./coverage",
    "md": "mos"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zkochan/codemo"
  },
  "keywords": [
    "example"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "z@kochan.io",
    "url": "https://www.kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zkochan/codemo/issues"
  },
  "homepage": "https://github.com/zkochan/codemo#readme",
  "mos": {
    "installation": {
      "useShortAlias": true
    }
  },
  "dependencies": {
    "acorn": "^5.2.1",
    "babel-core": "^6.26.0",
    "callsites": "^2.0.0",
    "cross-spawn-async": "^2.2.5",
    "file-position": "0.0.0",
    "lodash.partition": "^4.3.0",
    "normalize-newline": "^3.0.0",
    "normalize-path": "^2.1.1",
    "promise.prototype.finally": "^1.0.1",
    "rollup": "^0.51.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-includepaths": "^0.2.2",
    "source-map": "^0.6.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "core-js": "^2.4.0",
    "cz-conventional-changelog": "1.1.5",
    "eslint": "2.8.0",
    "eslint-config-standard": "5.1.0",
    "eslint-plugin-promise": "1.1.0",
    "eslint-plugin-standard": "1.3.2",
    "ghooks": "1.2.1",
    "istanbul": "^0.4.2",
    "mocha": "^4.0.1",
    "mos": "^1.2.0",
    "package-preview": "^1.0.0",
    "promise.prototype.finally": "^1.0.1",
    "validate-commit-msg": "2.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "node ./node_modules/validate-commit-msg/index.js",
      "pre-commit": "npm test"
    }
  }
}
